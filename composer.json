{
    "name": "octower/octower",
    "description": "Deployment Manager",
    "keywords": ["deployment", "composer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "William Pottier",
            "email": "developer@william-pottier.fr",
            "homepage": "http://william-pottier.fr"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "justinrainbow/json-schema": "4.0.*",
        "seld/jsonlint": "1.*",
        "symfony/console": "~2.5",
        "symfony/filesystem": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/process": "~2.5",
        "symfony/yaml": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.10"
    },
    "autoload": {
        "psr-0": { "Octower": "src/" }
    },
    "bin": ["bin/octower"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}